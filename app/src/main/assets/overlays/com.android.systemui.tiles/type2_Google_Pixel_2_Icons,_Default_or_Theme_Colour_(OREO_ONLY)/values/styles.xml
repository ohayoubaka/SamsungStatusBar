<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="systemui_theme_light" parent="@*android:style/Theme.Material.Light">
        <item name="android:colorPrimary">@*com.android.systemui:color/system_primary_color</item>
    </style>
    <style name="systemui_theme" parent="@*android:style/Theme.Material.Light">
    </style>
	<style name="systemui_theme_remote_input" parent="@*android:style/Theme.Material.Light">
        <item name="android:colorAccent">@*com.android.systemui:color/remote_input_accent</item>
    </style>
	<style name="dialogTheme" parent="@*android:style/Theme.Material.Light.Dialog" />
	<style name="TextAppearance.QS.DetailHeader" parent="@*com.android.systemui:style/TextAppearance.QS">
        <item name="android:textSize">18.5sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*com.android.systemui:color/qs_detail_header_title</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
	<style name="TextAppearance.QS.DetailButton" parent="@*com.android.systemui:style/TextAppearance.QS">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*com.android.systemui:color/qs_detail_action_button</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
    <style name="TextAppearance.Volume.Header" parent="@*com.android.systemui:style/TextAppearance.Volume">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@*com.android.systemui:color/tw_volume_title_color_light</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
</resources>
